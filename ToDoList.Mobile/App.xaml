             xmlns:local="clr-namespace:ToDoList.Mobile"
             x:Class="ToDoList.Mobile.App">
    <Application.Resources>
        <ResourceDictionary>

            <x:Array x:Key="ToDoItensMockData" Type="{x:Type x:String}">
